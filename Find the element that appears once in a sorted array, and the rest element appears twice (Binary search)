import java.util.ArrayList;

public class Solution 
{
    public static int uniqueElement(ArrayList<Integer> arr) 
    {
        //    Write your code here.
          int x=0;
        
        for(int i=0;i<arr.size();i++)
        {
            x=x^arr.get(i);
        }
        
        return x;
    }
}
